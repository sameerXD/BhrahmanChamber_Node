                        <th scope="col">Mobile No </th>
                        <th scope="col">companyName </th>
                        <!-- <th scope="col"> Date</th> -->
                        <th scope="col"> Plan</th>
                    </tr>
                </thead>
                <tbody>
                    <!-- email, name, mobileNumber, companyName, plan -->
                    <% for(var i=0; i < allusers.length; i++) { %>
                        <tr>
                            <th scope="row">
                                <%= [i+1] %>
                            </th>

                            <td>
                                <%= allusers[i].email %>
                            </td>
                           
                            <td>
